{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxBodySize": "200mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}